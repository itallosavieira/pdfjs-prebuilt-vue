{
  "name": "vue3-pdf-render",
  "private": false,
  "version": "0.1.0-alpha.2",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/itallosavieira/vue3-pdf-render"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-pdf-render.umd.js",
  "module": "./dist/vue3-pdf-render.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-pdf-render.es.js",
      "require": "./dist/vue3-pdf-render.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "keywords": [
    "pdf-viewer",
    "pdfjs",
    "vue3",
    "vue",
    "vuejs"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "exit 0"
  },
  "dependencies": {
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@types/node": "^20.11.6",
    "@vitejs/plugin-vue": "^4.5.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-css-injected-by-js": "^3.3.1",
    "vue-tsc": "^1.8.25"
  }
}
